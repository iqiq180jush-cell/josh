-- ============================================
-- 秒杀系统核心（简化版）
-- ============================================

-- 初始化秒杀系统
Module.FastAttack = (function()
    if _ENV.rz_FastAttack then return _ENV.rz_FastAttack end
    local FastAttack = {
        Distance = 10000,
        attackMobs = true,
        attackPlayers = true,
        IsRunning = _G.FastAttack
    }
    
    _ENV.rz_FastAttack = FastAttack
    return FastAttack
end)()

-- 启动秒杀系统循环
task.spawn(function()
    while true do
        if _G.FastAttack and _ENV.rz_FastAttack and _ENV.rz_FastAttack.IsRunning then
            -- 这里可以添加实际的攻击逻辑
            task.wait(Settings.ClickDelay)
        else
            task.wait(0.1)
        end
    end
end)

-- ============================================
-- 完成加载
-- ============================================

task.wait(0.5)

game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "GG脚本",
	Text = "加载完成 祝您使用愉快",
	Duration = 5;
})

print("=========================================")
print("GG脚本已完全加载！")
print("UI尺寸：500x420")
print("功能特点：")
print("- 视角缩放：滑动条输入")
print("- 秒杀设置：数字输入框")
print("- 移动速度：数字输入框")
print("- 所有原功能保留")
print("=========================================")
