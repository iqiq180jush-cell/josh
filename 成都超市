-- 移速功能
CreateLabel(tab3, "移动加速")
local speedTextbox = CreateTextbox(tab3, "输入移动速度", function(text)
    local speed = tonumber(text)
    if speed then
        print("移動速度設置為: " .. speed)
    end
end)

local moveAccelToggle = CreateToggle(tab3, "移动加速开关", false, function(state)
    print("移動加速: " .. tostring(state))
    
    if state then
        local player = game.Players.LocalPlayer
        local speed = 50
        
        local connection = game:GetService("RunService").Heartbeat:Connect(function()
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                local humanoid = player.Character.Humanoid
                if humanoid.MoveDirection.Magnitude > 0 then
                    local moveDirection = humanoid.MoveDirection
                    local acceleration = moveDirection * speed / 30
                    player.Character:TranslateBy(acceleration)
                end
            end
        end)
    end
end)

