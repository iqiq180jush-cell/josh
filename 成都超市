-- 飛行按鈕
CreateButton(tab2, "飞行", function()
    print("飛行功能已啟用")
    -- 飛行腳本的實現
    local flying = false
    local speed = 50
    local bodyGyro, bodyVelocity
    
    local function fly()
        local player = game.Players.LocalPlayer
        if not player.Character then return end
        
        local torso = player.Character:FindFirstChild("HumanoidRootPart")
        if not torso then return end
        
        bodyGyro = Instance.new("BodyGyro", torso)
        bodyVelocity = Instance.new("BodyVelocity", torso)
        
        bodyGyro.P = 9e4
        bodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
        bodyGyro.cframe = torso.CFrame
        
        bodyVelocity.velocity = Vector3.new(0, 0, 0)
        bodyVelocity.maxForce = Vector3.new(9e9, 9e9, 9e9)
        
        spawn(function()
            repeat wait()
                player.Character.Humanoid.PlatformStand = true
                if torso and bodyVelocity and bodyGyro then
                    local moveDirection = Vector3.new()
                    
                    if uis:IsKeyDown(Enum.KeyCode.W) then
                        moveDirection = moveDirection + Vector3.new(0, 0, -1)
                    end
                    if uis:IsKeyDown(Enum.KeyCode.S) then
                        moveDirection = moveDirection + Vector3.new(0, 0, 1)
                    end
                    if uis:IsKeyDown(Enum.KeyCode.A) then
                        moveDirection = moveDirection + Vector3.new(-1, 0, 0)
                    end
                    if uis:IsKeyDown(Enum.KeyCode.D) then
                        moveDirection = moveDirection + Vector3.new(1, 0, 0)
                    end
                    if uis:IsKeyDown(Enum.KeyCode.Space) then
                        moveDirection = moveDirection + Vector3.new(0, 1, 0)
                    end
                    if uis:IsKeyDown(Enum.KeyCode.LeftShift) then
                        moveDirection = moveDirection + Vector3.new(0, -1, 0)
                    end
                    
                    bodyVelocity.velocity = moveDirection * speed
                end
            until not flying
            
            if torso then
                if bodyGyro then bodyGyro:Destroy() end
                if bodyVelocity then bodyVelocity:Destroy() end
                player.Character.Humanoid.PlatformStand = false
            end
        end)
    end
    
    -- 創建飛行控制面板
    local flyPanel = Instance.new("Frame", game:GetService("CoreGui"))
    flyPanel.Size = UDim2.new(0, 200, 0, 100)
    flyPanel.Position = UDim2.new(0.5, -100, 0.8, 0)
    flyPanel.BackgroundColor3 = Color3.fromRGB(15, 17, 22)
    Instance.new("UICorner", flyPanel)
    
    local speedLabel = Instance.new("TextLabel", flyPanel)
    speedLabel.Size = UDim2.new(1, 0, 0.3, 0)
    speedLabel.Text = "飛行速度: " .. speed
    speedLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    speedLabel.BackgroundTransparency = 1
    
    local speedSlider = Instance.new("Frame", flyPanel)
    speedSlider.Size = UDim2.new(0.8, 0, 0, 6)
    speedSlider.Position = UDim2.new(0.1, 0, 0.5, 0)
    speedSlider.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    Instance.new("UICorner", speedSlider)
    
    local flyToggleBtn = Instance.new("TextButton", flyPanel)
    flyToggleBtn.Size = UDim2.new(0.8, 0, 0, 30)
    flyToggleBtn.Position = UDim2.new(0.1, 0, 0.7, 0)
    flyToggleBtn.Text = "關閉飛行"
    flyToggleBtn.BackgroundColor3 = Color3.fromRGB(30, 35, 45)
    flyToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    Instance.new("UICorner", flyToggleBtn)
    
    flyToggleBtn.MouseButton1Click:Connect(function()
        flying = false
        flyPanel:Destroy()
    end)
    
    flying = true
    fly()
end)

