-- ============================================
-- 秒杀系统核心（简化版）
-- ============================================

local function FastAttackSystem()
    -- 这里放置原秒杀系统的核心逻辑
    -- 由于代码很长，这里只放一个简化的版本
    task.spawn(function()
        while true do
            if _G.FastAttack and _ENV.rz_FastAttack and _ENV.rz_FastAttack.IsRunning then
                -- 模拟攻击逻辑
                task.wait(Settings.ClickDelay)
            else
                task.wait(0.1)
            end
        end
    end)
end

-- 初始化秒杀系统
Module.FastAttack = (function()
    if _ENV.rz_FastAttack then return _ENV.rz_FastAttack end
    local FastAttack = {
        Distance = 10000,
        attackMobs = true,
        attackPlayers = true,
        IsRunning = _G.FastAttack
    }
    
    _ENV.rz_FastAttack = FastAttack
    return FastAttack
end)()

-- 启动秒杀系统
task.spawn(FastAttackSystem)

-- ============================================
-- 完成加载
-- ============================================

task.wait(0.5)

game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "GG脚本",
	Text = "加载完成 祝您使用愉快",
	Duration = 5;
})

print("GG脚本加载完成！")
print("UI尺寸：450x380")
print("点击左上角GG按钮打开菜单")
