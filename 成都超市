CreateSection(UITab5, "传送功能")

CreateButton(UITab5, "传送至一海", function()
    print("尝试传送到一海")
    local success = pcall(function()
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelMain")
    end)
    if success then
        print("传送成功")
    else
        print("传送失败")
    end
end)

CreateButton(UITab5, "传送至二海", function()
    print("尝试传送到二海")
    local success = pcall(function()
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
    end)
    if success then
        print("传送成功")
    end
end)

CreateButton(UITab5, "传送至三海", function()
    print("尝试传送到三海")
    local success = pcall(function()
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
    end)
    if success then
        print("传送成功")
    end
end)

CreateSection(UITab5, "二海传送点")

CreateButton(UITab5, "天鹅的房间", function()
    print("传送到天鹅的房间")
    local player = game.Players.LocalPlayer
    local targetPosition = Vector3.new(-287.37, 305.81, 592.98)
    if player.Character then
        player.Character:MoveTo(targetPosition)
    end
end)

