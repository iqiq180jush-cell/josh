    end
end)

CreateButton(tab5, "传送至豪宅", function()
    print("傳送至豪宅")
    local player = game.Players.LocalPlayer
    if player.Character then
        local target = Vector3.new(2286.93, 15.06, 910.51)
        player.Character:MoveTo(target)
    end
end)

CreateButton(tab5, "传送至鬼船里", function()
