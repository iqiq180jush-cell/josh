local function CreateTab(name)

    local P = Instance.new("ScrollingFrame", Container); P.Size = UDim2.new(1, 0, 1, 0); P.BackgroundTransparency = 1; P.ScrollBarThickness = 2; P.Visible = false; P.AutomaticCanvasSize = "Y"

    Instance.new("UIListLayout", P).Padding = UDim.new(0, 10); Instance.new("UIListLayout", P).HorizontalAlignment = "Center"

   

    local B = Instance.new("TextButton", Side); B.Size = UDim2.new(1, 0, 0, 38); B.BackgroundColor3 = Color3.fromRGB(30, 30, 40); B.Text = name; B.TextColor3 = Color3.new(0.95, 0.95, 0.95); B.Font = "GothamBold"; B.TextSize = 11; Instance.new("UICorner", B)

    local BS = Instance.new("UIStroke", B); BS.Thickness = 1.5; table.insert(STROKES, BS)

    B.MouseButton1Click:Connect(function() for _, v in pairs(Tabs) do v.Visible = false end P.Visible = true end)

    table.insert(Tabs, P); return P

end



