-- [[ 5. é«˜æ€§èƒ½æ ¸å¿ƒçµ„ä»¶ (Toggle / Slider / Label) ]]

local function AddToggle(parent, text, flag, cb)

    local b = Instance.new("TextButton", parent); b.Size = UDim2.new(0.95, 0, 0, 42); b.BackgroundColor3 = Color3.fromRGB(45, 45, 55); b.Text = "  "..text.." [é—œé–‰]"; b.TextColor3 = Color3.new(1,1,1); b.Font = "GothamBold"; b.TextSize = 12; b.TextXAlignment = "Left"; Instance.new("UICorner", b)

    b.MouseButton1Click:Connect(function()

        getgenv().CaiHuaConfig.Toggles[flag] = not getgenv().CaiHuaConfig.Toggles[flag]

        local s = getgenv().CaiHuaConfig.Toggles[flag]

        b.Text = "  "..text..(s and " [é–‹å•Ÿ]" or " [é—œé–‰]"); b.BackgroundColor3 = s and Color3.fromRGB(200, 50, 50) or Color3.fromRGB(45, 45, 55)

        cb(s)

    end)

end



local function AddSlider(parent, text, min, max, default, flag, cb)

    local f = Instance.new("Frame", parent); f.Size = UDim2.new(0.95, 0, 0, 65); f.BackgroundTransparency = 1

    local l = Instance.new("TextLabel", f); l.Size = UDim2.new(1, 0, 0, 25); l.Text = "  "..text.." : "..default; l.TextColor3 = Color3.new(1,1,1); l.BackgroundTransparency = 1; l.Font = "GothamBold"; l.TextSize = 12; l.TextXAlignment = "Left"

    local s = Instance.new("TextButton", f); s.Size = UDim2.new(1, 0, 0, 8); s.Position = UDim2.new(0, 0, 0, 38); s.BackgroundColor3 = Color3.fromRGB(40, 40, 50); s.Text = ""; Instance.new("UICorner", s)

    local bar = Instance.new("Frame", s); bar.Size = UDim2.new((default-min)/(max-min), 0, 1, 0); bar.BackgroundColor3 = Color3.fromRGB(255, 0, 0); Instance.new("UICorner", bar)

    local function Update(input)

        local per = math.clamp((input.Position.X - s.AbsolutePosition.X) / s.AbsoluteSize.X, 0, 1)

        local val = math.floor(min + (max - min) * per)

        bar.Size = UDim2.new(per, 0, 1, 0); l.Text = "  "..text.." : "..val; getgenv().CaiHuaConfig.Settings[flag] = val; cb(val)

    end

    s.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then Update(i) end end)

end



