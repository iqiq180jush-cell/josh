    local l = Instance.new("TextLabel", T_Notice); l.Size = UDim2.new(0.95, 0, 0, 35); l.BackgroundTransparency = 1; l.Text = txt; l.TextColor3 = color or Color3.new(1,1,1); l.Font = font or "GothamBold"; l.TextSize = 12; l.TextWrapped = true; return l

end



AddNotice("--- ç§¦å§‹çš‡ V190 å…¬å‘Š ---", Color3.new(1, 0.9, 0))

AddNotice("åµæ¸¬ä¿®å¾©ï¼šè£ç½®ã€FPSã€Ping å¿…è·³", Color3.new(0.5, 1, 0.5))

