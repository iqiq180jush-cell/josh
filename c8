-- [[ 7. å…¨åŠŸèƒ½æ ¸å¿ƒé©…å‹•å¼•æ“ - è¶…é•·ä»£ç¢¼å¡«å…… ]]



-- ç©å®¶å¾ªç’° (è·‘é€Ÿ/è·³èº/é€è¦–)

S_RS.RenderStepped:Connect(function()

    pcall(function()

        if LP.Character and LP.Character:FindFirstChild("Humanoid") then

            if getgenv().CaiHuaConfig.Toggles.Speed then LP.Character.Humanoid.WalkSpeed = getgenv().CaiHuaConfig.Settings.WalkSpd end

            if getgenv().CaiHuaConfig.Toggles.Jump then LP.Character.Humanoid.JumpPower = getgenv().CaiHuaConfig.Settings.JumpPower end

        end

       

        -- è¦–è¦º ESP æ¸²æŸ“ (é«˜å†—é¤˜ç©©å®šç‰ˆ)

        if getgenv().CaiHuaConfig.Toggles.ESP_Main then

            for _, p in pairs(S_PL:GetPlayers()) do

                if p ~= LP and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then

                    local hrp = p.Character.HumanoidRootPart

                    local dist = LP:DistanceFromCharacter(hrp.Position)

                    if dist <= getgenv().CaiHuaConfig.Settings.ESPDistance then

                        if not hrp:FindFirstChild("CH_ESP") then

                            local bg = Instance.new("BillboardGui", hrp); bg.Name = "CH_ESP"; bg.AlwaysOnTop = true; bg.Size = UDim2.new(0, 120, 0, 60); bg.ExtentsOffset = Vector3.new(0, 3, 0)

                            local tl = Instance.new("TextLabel", bg); tl.Size = UDim2.new(1, 0, 1, 0); tl.BackgroundTransparency = 1; tl.TextColor3 = Color3.new(1, 1, 1); tl.Font = "GothamBold"; tl.TextSize = 10; tl.TextStrokeTransparency = 0

                        end

                        local str = ""

                        if getgenv().CaiHuaConfig.Toggles.ESP_Name then str = str .. p.DisplayName .. "\n" end

                        if getgenv().CaiHuaConfig.Toggles.ESP_Health then str = str .. "HP: " .. math.floor(p.Character.Humanoid.Health) .. "\n" end

                        if getgenv().CaiHuaConfig.Toggles.ESP_Dist then str = str .. math.floor(dist) .. "m" end

                        hrp.CH_ESP.TextLabel.Text = str; hrp.CH_ESP.Enabled = true

                    else

                        if hrp:FindFirstChild("CH_ESP") then hrp.CH_ESP.Enabled = false end

                    end

                end

            end

        else

            for _, p in pairs(S_PL:GetPlayers()) do pcall(function() p.Character.HumanoidRootPart.CH_ESP.Enabled = false end) end

        end

    end)

end)



-- æˆ°é¬¥æ®ºæˆ®ç³»çµ±

task.spawn(function()

    while true do

        task.wait(getgenv().CaiHuaConfig.Settings.AtkSpeed)

        if getgenv().CaiHuaConfig.Toggles.FastAttack then

            pcall(function()

                for _, v in pairs(workspace.Enemies:GetChildren()) do

                    if v:FindFirstChild("Head") and v.Humanoid.Health > 0 and LP:DistanceFromCharacter(v.Head.Position) <= getgenv().CaiHuaConfig.Settings.AtkRange then

                        S_REP.Modules.Net["RE/RegisterAttack"]:FireServer(0)

                        S_REP.Modules.Net["RE/RegisterHit"]:FireServer(v.Head, {{v, v.Head}})

                        break

                    end

                end

            end)

        end

    end

end)



